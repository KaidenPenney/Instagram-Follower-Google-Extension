{
    "manifest_version": 3,
    "name": "Instagram Not Following Back",
    "version": "1.0",
    "permissions": ["tabs", "activeTab", "scripting"],
    "host_permissions": ["https://www.instagram.com/*"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://www.instagram.com/*"],
            "js": ["content.js"]
        }
    ]
}